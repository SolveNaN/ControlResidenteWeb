<!-- ======= Contact Section ======= -->
<section id="contact" class="contact section-bg">
    <div class="container" data-aos="fade-up">

        <div class="section-title">
            <h2>@arear.Nombre</h2>
            <p>@arear.NumeroSolicitude</p>
        </div>

        <div class="row">

            <div class="col-lg-12">
                <img src=@arear.NombreImagen class="rounded mx-auto d-block" alt="..." style="width:100%">
                             

            </div>

            <div class="col-lg-12 mt-4 mt-md-0" style="overflow:auto">
               <TablaSolicitudes />
            </div>

        </div>

    </div>
</section><!-- End Contact Section -->
@code{
    [Parameter]
    public AreaComun? arear {get;set;}
}